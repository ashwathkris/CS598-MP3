namespace DataFrame;
enum ValueType: byte {
	Int,
	Float,
	String
}
table Metadata {
	name:string;
	dtype:ValueType;
}
table Column {
	metadata: Metadata;
	intval: [int64];
	floatval: [float64];
	stringval: [string];
}
table DataFrame {
  metadata: string;
  columns: [Column];
}
root_type DataFrame;